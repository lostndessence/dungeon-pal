<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lostkeys.dungeonpal" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Dungeon Pal</name>
    <description>
        A simple app to keep track of DnD 5e PC's
    </description>
    <author email="rolando@ad-in.us" href="http://phonegap.com">
        LostKeys
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
